pipeline{
   agent any
    stages{
      stage('git checkout'){
        steps{
          script{
             git 'https://github.com/kiranbdevops/maven_new.git'
                }
             }
                          }
		stage('unit testing'){
        steps{
         sh 'mvn test'
                }
             }

		stage('Integration testing'){
        steps{
         sh 'mvn verify -DskipUnitTests'
                }
             }

                          }
        }

         
